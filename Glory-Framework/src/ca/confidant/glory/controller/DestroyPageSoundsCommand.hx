package ca.confidant.glory.controller;    //import org.puremvc.as3.interfaces.ICommand;    import org.puremvc.haxe.interfaces.INotification;    import org.puremvc.haxe.patterns.command.SimpleCommand;	import ca.confidant.glory.view.SoundMediator;	import ca.confidant.glory.model.PagesConfigProxy;	import haxe.xml.Fast;	import nme.Assets;	/*	 * @author Allan Dowdeswell	 * Triggered by the RemovePageCommand. This removes sound instances and mediators associated with a page.	 */    class DestroyPageSoundsCommand extends SimpleCommand    {        override public function execute( note:INotification ) : Void        {			trace('DestroyPageSoundsCommand');			var pcp=cast(facade.retrieveProxy(PagesConfigProxy.NAME) , PagesConfigProxy);			var pageId=cast(note.getBody(),String);					var sounds=pcp.getPageSounds(pageId);						//var sounds=cast(note.getBody(),List<Dynamic>);			if (sounds.length>0){				for (thisSound in sounds){					trace("Removed sound:"+thisSound.att.id);					facade.removeMediator(thisSound.att.id);				}			}        }		    }