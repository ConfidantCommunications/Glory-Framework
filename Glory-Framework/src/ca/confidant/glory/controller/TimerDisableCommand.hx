package ca.confidant.glory.controller;    import org.puremvc.haxe.patterns.command.SimpleCommand;	import org.puremvc.haxe.interfaces.INotification;    import ca.confidant.glory.ApplicationFacade;	import ca.confidant.glory.ApplicationFacade;	import ca.confidant.glory.model.TickerProxy;	import ca.confidant.glory.model.PagesConfigProxy;/** * Use this command to disable the TickerProxy. */    class TimerDisableCommand extends SimpleCommand    {        override public function execute( note:INotification ) : Void        {			trace('TimerDisableCommand');			if(facade.hasProxy(TickerProxy.NAME)){				var p: TickerProxy;				p=cast(facade.retrieveProxy(TickerProxy.NAME),TickerProxy);				p.stopTimer();			}        }    }